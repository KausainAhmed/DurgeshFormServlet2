<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	id="WebApp_ID" version="4.0">
	<display-name>DurgeshFormServlet2</display-name>
	<welcome-file-list>
		<welcome-file>home.html</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>ser1</servlet-name>
		<servlet-class>com.durgesh.form.Serv1</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ser1</servlet-name>
		<url-pattern>/Serv1</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>ser2</servlet-name>
		<servlet-class>com.durgesh.form.Serv2</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ser2</servlet-name>
		<url-pattern>/Serv2</url-pattern>
	</servlet-mapping>
</web-app>